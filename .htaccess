AddType image/svg+xml svg svgz
AddEncoding gzip svgz
AddType application/x-httpd-php .php .php3 .htm .html
ErrorDocument 403 http://eits-demo.domain-account.com/_resources/error_docs/403/
ErrorDocument 404 http://eits-demo.domain-account.com/_resources/error_docs/404/
ErrorDocument 500 http://eits-demo.domain-account.com/_resources/error_docs/500/
<FilesMatch ".(eot|ttf|otf|woff)">
Header set Access-Control-Allow-Origin: http://a.cms.omniupdate.com
</FilesMatch>
<files .htaccess>
    order allow,deny
    deny from all
</files>
Options +FollowSymLinks
#########################
#  Begin Rewrite Rules
#########################
#
# Turn on Rewrite Engine
RewriteEngine on
RewriteBase /
#
# Turn on https
# RewriteCond %{HTTPS} off [NC]
# RewriteRule ^(.*)$ https://%{HTTP_HOST}%{REQUEST_URI} [R=301,NE,L]
#
# Remove the www
RewriteCond %{HTTP_HOST} ^www.eits-demo.domain-account.com$ [NC]
RewriteRule ^ http://eits-demo.domain-account.com%{REQUEST_URI} [R=301,L]
#
#
# Add trailing slash to url
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_URI} !(\.[a-zA-Z0-9]{1,5}|/|#(.*))$
RewriteRule ^(.*)$ $1/ [R=301,L]
#
# Remove .html extension from url
RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_FILENAME}\.html -f
RewriteRule ^([^\.]+)/$ $1.html
#
# Remove slash from the URL
RewriteCond %{THE_REQUEST} ^[A-Z]{3,9}\ /.*\.html\ HTTP/
RewriteRule ^(.*)\.html$ /$1 [R=301,L]
#
#
RewriteRule ^index\.html$ / [R=301,L]
RewriteRule ^(.*)/index\.html$ /$1/ [R=301,L]
